{
    "rules": [
      {
        "enabled": true,
        "name": "deleteAfterOneHour",
        "type": "Lifecycle",
        "definition": {
          "actions": {
            "baseBlob": {
              "delete": {
                "daysAfterModificationGreaterThan": 0.0417
              }
            }
          },
          "filters": {
            "blobTypes": [
              "blockBlob"
            ],
            "prefixMatch": [
              "fileshare/"
            ]
          }
        }
      }
    ]
  }